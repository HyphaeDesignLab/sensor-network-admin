{
  "name": "hyphae-geo-mvp-dashboard",
  "version": "1.0.0",
  "description": "the dashboard where Geo MVP projects are assembled, administered, published",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "start": "nodemon server/index.js",
    "build:dev": "npm run client-env dev && webpack --mode=development",
    "build:devprod": "npm run client-env prod && webpack --mode=development",
    "build:prod": "npm run client-env prod && webpack --mode=production --node-env=production",
    "proxy-api": "lcp --proxyUrl https://smai.us --proxyPartial smai-prod",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "client-env": "./setenv.sh $1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanecat/hyphae-ecosystemhealth-mvp.git"
  },
  "author": "Ivan Velev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vanecat/hyphae-ecosystemhealth-mvp/issues"
  },
  "homepage": "https://github.com/vanecat/hyphae-ecosystemhealth-mvp#readme",
  "dependencies": {
    "axios": "^0.21.4",
    "firebase": "^9.6.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.29.0",
    "react-router-dom": "^6.0.1",
    "styled-components": "^5.3.3",
    "mapbox-gl": "^2.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@webpack-cli/generators": "^0.1.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1",
    "style-loader": "^3.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.0"
  }
}
